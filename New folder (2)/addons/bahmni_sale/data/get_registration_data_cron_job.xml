<?xml version="1.0" encoding="utf-8"?>
<odoo>
<!--    This cron job is developed for getting registration data from OpenMRS+ side -->
    <data noupdate="1">
        <record id="ir_cron_module_get_registration_data" model="ir.cron">
            <field name="name">Registration Data</field>
            <field eval="False" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
            <field eval="'sale.order'" name="model" />
            <field eval="'get_registration_data'" name="function" />
            <field eval="'()'" name="args" />
            <field name="priority">5</field>
        </record>
    </data>
</odoo>