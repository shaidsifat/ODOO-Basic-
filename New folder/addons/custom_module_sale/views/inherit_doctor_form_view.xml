<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="inherit_hospital_doctor_form_view" model="ir.ui.view">
        <field name="name">doctor.profile.inherit.form.view</field>
        <field name="model">doctor.profile</field>
        <field name="inherit_id" ref="hospital.hospital_doctor_user_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='institution']" position="after">
                <field name="doctor_ref" />
            </xpath>
        </field>
    </record>


